{
    "version": 2,
    "env": {
      "DENO_VERSION": "1.0.0"
    },
    "functions": {
      "api/**/*.[jt]s": {
        "runtime": "vercel-deno@0.0.4"
      }
    },
    "routes": [
        {
            "src": "/",
            "headers": {"cache-control": "s-maxage=86400, stale-while-revalidate"},
            "dest": "index.html"
        },
        {
            "src": "/(?<vimeoId>[^/]*)_small.jpg",
            "headers": {"cache-control": "s-maxage=86400, stale-while-revalidate"},
            "dest": "/api/video.js?id=$vimeoId&key=thumbnail_small"
        },
        {
            "src": "/(?<vimeoId>[^/]*)_medium.jpg",
            "headers": {"cache-control": "s-maxage=86400, stale-while-revalidate"},
            "dest": "/api/video.js?id=$vimeoId&key=thumbnail_medium"
        },
        {
            "src": "/(?<vimeoId>[^/]*)_large.jpg",
            "headers": {"cache-control": "s-maxage=86400, stale-while-revalidate"},
            "dest": "/api/video.js?id=$vimeoId&key=thumbnail_large"
        },
        {
            "src": "/(?<vimeoId>[^/]*).jpg",
            "headers": {"cache-control": "s-maxage=86400, stale-while-revalidate"},
            "dest": "/api/video.js?id=$vimeoId&key=thumbnail_large"
        },
        {
            "src": "/(?<vimeoId>[^/]*)",
            "headers": {"cache-control": "s-maxage=86400, stale-while-revalidate"},
            "dest": "/api/video.js?id=$vimeoId"
        }
    ]
}
